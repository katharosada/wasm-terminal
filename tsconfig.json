{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "lib": ["dom", "es2015", "es2016", "webworker"],
      "moduleResolution": "node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "experimentalDecorators": true,
      "jsx": "react",
      "resolveJsonModule": true,
      "skipDefaultLibCheck": true,
      "skipLibCheck": true,
      "outDir": "lib",
      "baseUrl": ".",
    },
    "exclude": [
      "node_modules",
    ],
    "rootDir": "src",
  }
  